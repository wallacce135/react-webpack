{
    "compilerOptions": {
        "target": "ES6",
        "allowJs": true,
        "module": "Node16",
        "strict": true,
        "moduleResolution": "Node16",
        "isolatedModules": true,
        "noEmit": false,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "allowSyntheticDefaultImports": true,
        "jsx": "react-jsx",
        "outDir": "dist"
    },
    "include": ["**/*.tsx", "**/*.tsx"]
}